{
  "redirects": [
    {
      "source": "/community/:path*",
      "destination": "/community.html",
      "permanent": false
    },
    {
      "source": "/asset/:path*", 
      "destination": "/asset.html",
      "permanent": false
    }
  ],
  "rewrites": [
    {
      "source": "/community.html/:path*",
      "destination": "/community.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options", 
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}